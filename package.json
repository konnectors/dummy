{
  "name": "dummy",
  "version": "1.1.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:konnectors/dummy.git"
  },
  "keywords": [],
  "author": "Cozy Cloud",
  "license": "AGPL-3.0",
  "eslintConfig": {
    "extends": [
      "cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "cozy-konnector-dev",
    "standalone": "cozy-konnector-standalone",
    "pretest": "npm run clean",
    "clean": "rm -rf ./data build* ",
    "build": "webpack",
    "postbuild": "scripts/postbuild.sh",
    "lint": "eslint --fix src/ flavours/",
    "deploy": "scripts/deploy.sh",
    "cozyPublish": "cd build-twofa && cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build_twofa})",
    "travisDeployKey": "./bin/generate_travis_deploy_key"
  },
  "dependencies": {
    "cozy-konnector-libs": "4.56.4"
  },
  "devDependencies": {
    "cozy-jobs-cli": "1.20.2",
    "cozy-konnector-build": "1.4.5"
  }
}
